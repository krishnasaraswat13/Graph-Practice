class Solution {
    public List<List<Integer>> printGraph(int V, int edges[][]) {
         List<List<Integer>>al=new ArrayList<>();
         for(int i=0;i<V;i++){
             al.add(new ArrayList<>());
         }
         for(int i=0;i<edges.length;i++){
              int x=edges[i][0];
              int y=edges[i][1];
              al.get(x).add(y);
              al.get(y).add(x);
         }
         return al;
    }
}